<div class="professor_info">
  <h1><%= @professor.name %></h1>
  <h4>age: <%= @professor.age %></h4>
  <h4>specialty: <%= @professor.specialty %></h4>
</div>

<div class="student_info">
  <h4>avreage student age: <%= "#{@average_age}" %></h4>
  <% @students.each do |student| %>
    <h4 id="student_<%= student.id %>"> <%= student.name %> <%= link_to "Unenroll", "/professor_students/destroy?professor_id=#{@professor.id};student_id=#{student.id}", method: :delete, id: "unenroll_student_#{student.id}" %></h4>
  <%end%>
</div>
